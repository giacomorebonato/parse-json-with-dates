{
  "name": "parse-json-with-dates",
  "version": "0.1.1",
  "description": "Parse JSON casting date fields",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@types/jest": "^26.0.19",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "eslint": "^7.17.0",
    "husky": "^4.3.7",
    "istanbul-badges-readme": "^1.2.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rollup": "^2.35.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.4.4",
    "typedoc": "^0.20.13",
    "typedoc-plugin-markdown": "^3.4.0",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "build": "rollup -c",
    "clean": "rm -rf docs",
    "lint": "eslint ./src --ext .ts,.tsx",
    "make-badges": "istanbul-badges-readme",
    "prettier": "prettier ./src --write",
    "test": "jest --coverage",
    "typedoc": "npm run clean && typedoc --plugin typedoc-plugin-markdown --out docs src/index.ts"
  },
  "lint-staged": {
    "*.{js,jsx,json,ts,tsx}": ["prettier --write", "git add"]
  },
  "keywords": [
    "json",
    "parsing",
    "dates",
    "cast"
  ],
  "author": "Giacomo Rebonato",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && npm run lint && npm run prettier && npm run make-badges && git add 'README.md'"
    }
  }
}
